{
  "name": "dag-history-component",
  "version": "0.4.3",
  "description": "A React Component for Dag-History Visualization",
  "main": "lib/index.js",
  "scripts": {
    "webpack:server": "webpack-dev-server --host 0.0.0.0 --hot --inline --history-api-fallback --publicPath ./",
    "lint": "eslint '{src,test,example}/**/*.ts'",
    "tsc": "tsc",
    "sass": "node-sass src/daghistory.scss --output lib",
    "copysass": "cpx 'src/**/*.scss' lib",
    "mocha:coverage": "nyc ./node_modules/.bin/_mocha",
    "mocha": "mocha",
    "test": "npm-run-all mocha:coverage lint --parallel tsc sass copysass",
    "watch:tsc": "npm run tsc -- -w",
    "watch:sass": "npm run sass -- --watch",
    "watch:mocha": "npm run mocha -- -w",
    "watch:copysass": "npm run copysass -- --watch",
    "watch": "npm-run-all --parallel 'watch:*'",
    "develop": "npm-run-all --parallel watch webpack:server storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "npm run develop"
  },
  "author": "Chris Trevino <chris.trevino@atsid.com>",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib/"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.33.0",
    "@types/bluebird": "^3.0.36",
    "@types/chai": "^3.4.34",
    "@types/enzyme": "^2.5.39",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.33",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/redux-logger": "^2.6.32",
    "@types/redux-thunk": "^2.1.32",
    "autoprefixer": "^6.5.1",
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "cpx": "^1.3.1",
    "css-loader": "^0.25.0",
    "electron": "^1.4.10",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.9.0",
    "filesaver.js": "^0.2.0",
    "imports-loader": "^0.6.5",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-lint": "^1.8.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.2.2",
    "ts-node": "^1.7.0",
    "wallaby-webpack": "0.0.23",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "dependencies": {
    "@types/classnames": "^0.0.32",
    "@types/node": "^6.0.51",
    "@types/react": "^0.14.51",
    "@types/react-redux": "^4.4.33",
    "@types/react-tabs": "^0.5.21",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^0.8.33",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "lodash": "^4.17.2",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-icons": "^2.2.1",
    "react-keydown": "^1.6.2",
    "react-redux": "^4.4.0",
    "react-simple-dropdown": "^1.1.4",
    "react-tabs": "^0.7.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.12.0",
    "redux-dag-history": "^1.6.0",
    "typescript": "2.1.3-insiders.20161130",
    "typescript-eslint-parser": "^1.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "lib/",
      "stories/"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
