{
  "name": "dag-history-component",
  "version": "0.3.1",
  "description": "A React Component for Dag-History Visualization",
  "main": "lib/index.js",
  "scripts": {
    "webpack:server": "webpack-dev-server --hot --inline --history-api-fallback --publicPath ./",
    "babel": "babel src --out-dir lib",
    "lint": "esw 'src/**/*.{js,jsx}' 'test/**/*.{js,jsx}' 'stories/**/*.{js,jsx}' 'example/**/*.{js,jsx}' --color",
    "sass": "node-sass src/daghistory.scss --output lib",
    "copysass": "cpx 'src/**/*.scss' lib",
    "mocha": "mocha",
    "mocha:coverage": "NODE_ENV=test nyc --all mocha",
    "test": "npm-run-all lint mocha:coverage --parallel babel sass copysass",
    "watch:lint": "npm run lint -- -w",
    "watch:babel": "npm run babel -- --watch",
    "watch:sass": "npm run sass -- --watch",
    "watch:mocha": "npm run mocha -- -w",
    "watch:copysass": "npm run copysass -- --watch",
    "watch": "npm-run-all --parallel 'watch:*'",
    "develop": "npm-run-all --parallel watch webpack:server storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "npm run develop"
  },
  "author": "Chris Trevino <chris.trevino@atsid.com>",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib/"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "cpx": "^1.3.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.13",
    "file-loader": "^0.9.0",
    "filesaver.js": "^0.2.0",
    "imports-loader": "^0.6.5",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.1.0",
    "nyc": "^8.3.0",
    "phantomjs2-ext": "^0.2.2",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-lint": "^1.8.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "wallaby-webpack": "0.0.23",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.1.0",
    "redux-actions": "^0.12.0",
    "debug": "^2.2.0",
    "react-icons": "^2.2.1",
    "react-keydown": "^1.6.2",
    "react-redux": "^4.4.0",
    "react-simple-dropdown": "^1.1.4",
    "react-tabs": "^0.7.0",
    "redux": "^3.3.1",
    "redux-dag-history": "^1.5.0"
  }
}
