{
  "name": "dag-history-component",
  "version": "0.2.0",
  "description": "A React Component for Dag-History Visualization",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback --publicPath ./",
    "babel": "babel src --out-dir lib",
    "lint": "esw 'src/**/*.{js,jsx}' 'example/**/*.{js,jsx}' --color",
    "sass": "node-sass src/daghistory.sass --output lib",
    "copysass": "cpx 'src/**/*.{sass,scss}' lib",
    "mocha": "mocha --require babel-core/register 'src/**/*.spec.*'",
    "test": "npm-run-all lint --parallel babel sass copysass",
    "watch:lint": "npm run lint -- -w",
    "watch:babel": "npm run babel -- --watch",
    "watch:sass": "npm run sass -- --watch",
    "watch:mocha": "npm run mocha -- -w",
    "watch:copysass": "npm run copysass -- --watch",
    "watch": "npm-run-all --parallel 'watch:*'",
    "develop": "npm-run-all --parallel watch start"
  },
  "author": "Chris Trevino <chris.trevino@atsid.com>",
  "license": "ISC",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib/"
  ],
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2016": "^6.11.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "carte-blanche": "^0.2.3",
    "carte-blanche-react-plugin": "^0.2.2",
    "carte-blanche-source-plugin": "^0.1.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.13",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "phantomjs2-ext": "^0.2.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.3.1",
    "redux-actions": "^0.10.0",
    "redux-dag-history": "^1.2.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-lint": "^1.8.2",
    "sass-loader": "^4.0.0",
    "sasslint-webpack-plugin": "^1.0.3",
    "style-loader": "^0.13.1",
    "treeify": "^1.0.1",
    "wallaby-webpack": "0.0.22",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "peerDependencies": {
    "redux-dag-history": "^1.x"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "react-icons": "^2.2.1",
    "react-simple-dropdown": "^1.1.4",
    "react-tabs": "^0.7.0",
    "webpack-dev-server": "^1.14.1"
  }
}
